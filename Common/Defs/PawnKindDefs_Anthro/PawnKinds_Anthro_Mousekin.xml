		<combatPower>20</combatPower>
		<baseRecruitDifficulty>0.40</baseRecruitDifficulty>
		<isFighter>false</isFighter>
		<!-- Most Mousekins (or their unseen world pawn relatives) are assumed to be from the Kingdom faction -->
		<defaultFactionType>Mousekin_FactionKingdom</defaultFactionType>
		<invNutrition>2</invNutrition>
		<apparelIgnoreSeasons>true</apparelIgnoreSeasons>
		<forceNormalGearQuality>true</forceNormalGearQuality>
		<backstoryCryptosleepCommonality>0</backstoryCryptosleepCommonality>
		<apparelColor>(255,255,255)</apparelColor>
	</PawnKindDef>

