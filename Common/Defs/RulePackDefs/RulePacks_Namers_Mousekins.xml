<?xml version="1.0" encoding="utf-8"?>
<Defs>

	<RulePackDef>
		<defName>MousekinNamePackUtility</defName>
		<rulePack>
			<rulesStrings>
				<li>Mousekin_FirstName_Male->[mousekin_firstname_male]</li>
				<li>Mousekin_FirstName_Female->[mousekin_firstname_female]</li>
				<li>Mousekin_LastName_Commoner->[mousekin_surname_commoner]</li>
				<li>Mousekin_LastName_Exotic->[mousekin_surname_exotic]</li>
			</rulesStrings>
			<rulesFiles>
				<li>mousekin_firstname_male->Names/Mousekin_NamePack_Firstname_Male</li>
				<li>mousekin_firstname_female->Names/Mousekin_NamePack_Firstname_Female</li>
				<li>mousekin_surname_commoner->Names/Mousekin_NamePack_Surname_Commoner</li>
				<li>mousekin_surname_exotic->Names/Mousekin_NamePack_Surname_Exotic</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
	
	<RulePackDef>
		<defName>NamerPerson_MousekinMale</defName>
		<include>
			<li>MousekinNamePackUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name(p=5)->[Mousekin_FirstName_Male] [Mousekin_LastName_Commoner]</li>
				<li>r_name(p=1)->[Mousekin_FirstName_Male] [Mousekin_LastName_Exotic]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	
	<RulePackDef>
		<defName>NamerPerson_MousekinFemale</defName>
		<include>
			<li>MousekinNamePackUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name(p=5)->[Mousekin_FirstName_Female] [Mousekin_LastName_Commoner]</li>
				<li>r_name(p=1)->[Mousekin_FirstName_Female] [Mousekin_LastName_Exotic]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	
</Defs>
